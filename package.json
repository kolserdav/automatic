{
  "name": "automatic",
  "version": "1.1.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_ENV=production next build",
    "start": "NODE_ENV=production next start",
    "createtables": "node scripts/createTables.js",
    "prod": "echo logfile /home/spec/logs/out-${npm_package_version}.log > ~/.screenrc && screen -dmS node -L yarn start",
    "deploy": "yarn createtables && yarn build && yarn prod",
    "logs": "cat ~/logs/out-${npm_package_version}.log",
    "get": "screen -x node",
    "deletedemails": "node scripts/getDeletedEmails.js",
    "keys": "node scripts/getKeys.js",
    "accesslog": "sudo cat /var/log/nginx/access.log",
    "errorlog": "sudo cat /var/log/nginx/error.log"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "next": "9.5.5",
    "nodemailer": "^6.4.14",
    "query-string": "^6.13.6",
    "react": "17.0.1",
    "react-cookie": "^4.0.3",
    "react-dom": "17.0.1",
    "react-google-recaptcha": "^2.1.0",
    "react-google-recaptcha-v3": "^1.7.0",
    "sqlite3": "^5.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/cors": "^2.8.8",
    "@types/node": "^14.14.2",
    "@types/nodemailer": "^6.4.0",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.53",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/sqlite3": "^3.1.6",
    "sass": "^1.27.0",
    "typescript": "^4.0.3"
  }
}
